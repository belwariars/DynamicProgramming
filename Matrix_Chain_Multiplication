#include <bits/stdc++.h>
using namespace std;

void matrixChainMul(int arr[], int n)
{
	int table[n][n];
	
	int i,j,k,l,q;
	
	for(i=1; i<n; i++)
	{
		table[i][i] = 0; // no cost to get a individual matrix
	}
	
	for(l=2; l<n; l++)
	{
		for(i=1; i<n-l+1; i++)
		{
			j = i+l-1;
			
			table[i][j] = INT_MAX;
		
			for(k=i; k<=j-1; k++)
			{
				q = table[i][k] + table[k+1][j] + arr[i-1]*arr[k]*arr[j];
				if(q< table[i][j])
				{
					table[i][j] = q;
				}
			}
		}
	}
	
	printf("Maximum no. of Multiplications = %d", table[1][n-1]);
}

int main()
{
	int arr[] = {1, 2, 3, 4};
	int n = sizeof(arr)/sizeof(arr[0]);
	
	matrixChainMul(arr, n);
	return 0;
}
